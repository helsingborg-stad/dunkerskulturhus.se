{
    "name": "helsingborg-stad/dunkerskulturhus.se",
    "license": "GNU",
    "authors": [
        {
            "name": "Helsingborg Stad",
            "email": "webbredaktionen@helsingborg.se"
        }
    ],
    "minimum-stability" : "stable",
    "repositories": [
        {
            "type":"composer",
            "url":"https://packagist.org"
        },
        {
            "type":"composer",
            "url":"https://wpackagist.org"
        }
    ],

    "require": {
        "composer/installers": "~1.0",
        "johnpbloch/wordpress": "^5.0.2",
        "helsingborg-stad/modularity": ">=1.0.0",
        "helsingborg-stad/municipio": ">=1.0.0",
        "helsingborg-stad/lix-calculator": ">=1.0.1",
        "helsingborg-stad/open-hours": ">=1.0.0",
        "helsingborg-stad/custom-short-links": ">=1.0.0",
        "helsingborg-stad/content-scheduler": ">=1.0.0",
        "helsingborg-stad/force-ssl": ">=1.0.0",
        "wpackagist-plugin/wp-editor-widget": "^0.5.5",
        "wpackagist-plugin/varnish-http-purge": ">=4.3.1",
        "wpackagist-plugin/wp-nested-pages": "^1.5.4",
        "wpackagist-plugin/rest-api": "2.0-beta13",
        "helsingborg-stad/api-event-manager-integration": "0.6.13",
        "helsingborg-stad/google-analythics": "^1.0",
        "wpackagist-plugin/gravity-forms-no-captcha-recaptcha":"1.0.7",
        "wpackagist-plugin/redis-cache":"1.3.8",
        "helsingborg-stad/media-usage": ">=1.2.6",
        "wpackagist-plugin/autodescription": "^3.2"
    },

    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "wp-content/plugins/{$name}": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}": ["type:wordpress-theme"]
        }
    }
}
